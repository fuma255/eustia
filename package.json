{
  "name": "eustia",
  "version": "1.0.0",
  "description": "Bundler designed for utilities.",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write *.js src/**/*.js",
    "lint": "eslint *.js src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liriliri/eustia.git"
  },
  "keywords": [
    "eustia",
    "licia",
    "bundler",
    "utility"
  ],
  "author": "redhoodsu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liriliri/eustia/issues"
  },
  "homepage": "https://github.com/liriliri/eustia#readme",
  "devDependencies": {
    "eslint": "^4.19.1",
    "prettier": "^1.13.5"
  },
  "bin": {
    "eustia": "./bin/eustia.js"
  },
  "eustia": {
    "files": [
      "*.js",
      "src/**/*.js"
    ],
    "output": "src/lib/util.js",
    "format": "commonjs"
  }
}
