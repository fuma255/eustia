{
  "name": "eustia",
  "version": "1.0.0",
  "description": "Bundler designed for utilities.",
  "main": "out/index.js",
  "scripts": {
    "build": "gulp build",
    "dev": "gulp dev",
    "format": "prettier --write src/**/*.ts bin/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liriliri/eustia.git"
  },
  "keywords": [
    "eustia",
    "licia",
    "bundler",
    "utility"
  ],
  "author": "redhoodsu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liriliri/eustia/issues"
  },
  "homepage": "https://github.com/liriliri/eustia#readme",
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^5.0.35",
    "@types/lodash": "^4.14.110",
    "@types/node": "^10.5.1",
    "gulp": "^3.9.1",
    "gulp-typescript": "^5.0.0-alpha.3",
    "lodash": "^4.17.10",
    "prettier": "^1.13.5",
    "typescript": "^2.9.2"
  },
  "bin": {
    "eustia": "./bin/eustia.js"
  },
  "eustia": {
    "files": [
      "*.js",
      "src/**/*.js"
    ],
    "output": "src/lib/util.js",
    "format": "commonjs"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.16.0",
    "fs-extra": "^6.0.1",
    "glob": "^7.1.2"
  }
}
